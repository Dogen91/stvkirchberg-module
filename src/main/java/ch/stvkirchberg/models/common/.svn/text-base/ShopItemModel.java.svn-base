package ch.stvkirchberg.models.common;

public class ShopItemModel {

    private String article;

    private String cut;

    private String size;

    private String sizeMan;

    private String sizeLady;

    private int amount;

    public String getArticle() {
        return article;
    }

    public void setArticle(String article) {
        this.article = article;
    }

    public String getCut() {
        return cut;
    }

    public void setCut(String cut) {
        this.cut = cut;
    }

    public String getSize() {
        return size;
    }

    public void setSize(String size) {
        this.size = size;
    }

    public int getAmount() {
        return amount;
    }

    public void setAmount(int amount) {
        this.amount = amount;
    }

    public String getSizeMan() {
        return sizeMan;
    }

    public void setSizeMan(String sizeMan) {
        this.sizeMan = sizeMan;
    }

    public String getSizeLady() {
        return sizeLady;
    }

    public void setSizeLady(String sizeLady) {
        this.sizeLady = sizeLady;
    }

}
